);
function initHideMenuItem() {
	var loggedin = '{module_isloggedin}';
	if (loggedin == 1){
		jQuery('#topLinks .links ul li.login-link').hide();
	}
	else {
		jQuery('#topLinks .links ul li.login-link').show();
	}
}
</script>
